{
  "name": "upload-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "sst start",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "lint": "tsc && eslint '**/*.js'",
    "clean": "rm -rf docs node_modules pnpm-lock.yaml packages/*/{pnpm-lock.yaml,coverage,.nyc_output,dist,node_modules,.next}",
    "test": "npm test -w api"
  },
  "devDependencies": {
    "@ipld/dag-ucan": "^2.0.1",
    "@serverless-stack/cli": "^1.18.0",
    "@serverless-stack/resources": "^1.18.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.108",
    "ava": "^4.3.3",
    "aws-cdk-lib": "2.50.0",
    "hd-scripts": "^3.0.2",
    "lint-staged": "^13.0.3",
    "nanoid": "^4.0.0",
    "testcontainers": "^8.13.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.211.0",
    "@aws-sdk/client-s3": "^3.211.0",
    "@aws-sdk/util-dynamodb": "^3.211.0",
    "@serverless-stack/node": "^1.18.0",
    "@ucanto/interface": "^3.0.1",
    "@ucanto/principal": "^3.0.1",
    "@ucanto/server": "^3.0.4",
    "@ucanto/transport": "^3.0.2",
    "@web3-storage/access": "^5.0.1",
    "@web3-storage/sigv4": "^1.0.2",
    "multiformats": "^10.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/hd-scripts/eslint/index.js"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-console": "off",
      "no-warning-comments": "off"
    }
  },
  "workspaces": [
    "api"
  ]
}
